{
  "name": "loklok-lens-app-sst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 3001",
    "lint": "next lint",
    "start": "next start",
    "unlock": "npx sst unlock --stage development",
    "sst": "npm run unlock; npx sst dev --stage development",
    "sst:load:all": "npm run sst:load:dev; npm run sst:load:preview; npm run sst:load:production;",
    "sst:load:dev": "npx sst secret load .env.local --stage development",
    "sst:load:preview": "npx sst secret load .env.local --stage preview",
    "sst:load:production": "npx sst secret load .env.local --stage production",
    "sst:deploy:preview": "npx sst deploy --stage preview",
    "sst:deploy:production": "npx sst deploy --stage production",
    "sst:remove:prod:forever": "",
    "sst:remove:preview:forever": "npx sst remove --stage preview",
    "sst:remove:dev": "npx sst dev --stage development"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.749.0",
    "@aws-sdk/client-dynamodb": "^3.749.0",
    "@aws-sdk/util-dynamodb": "^3.749.0",
    "@types/md5": "^2.3.5",
    "copy-to-clipboard": "^3.3.3",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "jose": "^5.9.6",
    "md5": "^2.3.0",
    "next": "14",
    "raw-loader": "^4.0.2",
    "react": "18",
    "react-dom": "18",
    "reconnecting-websocket": "^4.4.0",
    "sst": "^3.9.2",
    "url-loader": "^4.1.1",
    "uuid": "^11.0.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
